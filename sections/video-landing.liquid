      <h1 data-aos="fade-up" data-aos-delay="100" data-aos-duration="1000" style="color:{{section.settings.header_color}}" class="h1 s9videohero__title {%- if section.settings.left -%}--left{%endif%}">{{section.settings.header_title}}
