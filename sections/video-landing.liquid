        "id": "description_color",
        "type": "color",
		"label":"Description Color",
        "default": "#000"
      },
      {
        "id": "button_link",
        "type": "url",
		"label":"Link to a collection/product/page"
      },
      {
        "id": "header_title",
        "type": "text",
        "label": "Make a statement",
        "default": "Lorem Ipsum"
      },
      {
        "type": "checkbox",
        "id": "left",
        "label": "Align the CTA to the left",
        "default": false
      },
      {
        "id": "description",
        "type": "text",
        "label": "Leave a description about your offer",
        "default": "Lorem ipsum dolor sit amet consectetur adipisicing elit. Fugiat ullam explicabo error ratione ipsa similique earum pariatur."
      },
      {
        "id": "copy_text",
        "type": "checkbox",
        "label": "Check for overlay text and CTA"      
      }
    ],
    "presets": [
      {
        "name": "Full Screen Video Landing w/ Floating Logo",
        "category": "Custom"
      }
    ]
  }
{% endschema %}