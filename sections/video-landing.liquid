<section class="showcase ">

  <video src="{{ section.settings.custom_video_url }}" playsinline muted loop autoplay></video>

  <div class="overlay"> </div>
  <div style="z-index:7;" class="s9hero__container {%- if section.settings.left -%}--left{%endif%}">
    {%- if section.settings.copy_text -%}
