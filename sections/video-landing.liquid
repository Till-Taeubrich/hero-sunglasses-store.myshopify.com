        <p class="s9hero__description_paragraph{%- if section.settings.left -%}--left{%endif%}" style="color:{{section.settings.description_color}}; text-align:center;">{{section.settings.description}}</p>
      </div>
      <a class="button s9hero_cta{%- if section.settings.left -%}--left{%endif%}" href="{{section.settings.button_link}}">SHOP</a>
    </div>
    {%- endif -%}
    
  </div>
